{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com"
    },
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://.*\\.github\\.io"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Authorization": "token $GITHUB_TOKEN"
      }
    }
  ],
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "10s",
  "timeout": "30s"
}
