{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://.*\\.github\\.io"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "retryOn429": true,
  "retryCount": 2,
  "fallbackRetryDelay": "3s",
  "timeout": "15s"
}
