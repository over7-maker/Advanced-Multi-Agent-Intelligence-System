{
  "dashboard": {
    "title": "AMAS Agent Performance Dashboard",
    "tags": ["amas", "agents", "performance"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 38,
    "version": 1,
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "enable": true,
          "expr": "changes(amas_slo_violations_total[5m]) > 0",
          "iconColor": "red",
          "name": "SLO Violations",
          "showIn": 0,
          "step": "5m",
          "titleFormat": "SLO Violation",
          "type": "tags",
          "useValueForTime": false
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Agent Request Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(amas_agent_requests_total[5m])",
            "legendFormat": "{{agent_id}} - {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 2,
        "title": "Agent Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(rate(amas_agent_requests_total{status=\"success\"}[5m]) / rate(amas_agent_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Agent Latency Distribution",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(amas_agent_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(amas_agent_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(amas_agent_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "Tool Call Performance",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum by (tool_name, status) (rate(amas_tool_call_duration_seconds_count[5m]))",
            "format": "table",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Token Usage by Agent",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(amas_tokens_used_total[5m])",
            "legendFormat": "{{agent_id}} - {{model_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens/sec",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Cost Tracking",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "increase(amas_cost_usd_total[1h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      }
    ]
  },
  "slo_dashboard": {
    "title": "AMAS SLO Monitoring Dashboard",
    "tags": ["amas", "slo", "reliability"],
    "timezone": "browser",
    "refresh": "1m",
    "schemaVersion": 38,
    "version": 1,
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "enable": true,
          "expr": "amas_slo_error_budget_remaining_percent < 10",
          "iconColor": "red",
          "name": "Error Budget Critical",
          "showIn": 0,
          "step": "1m",
          "titleFormat": "Error Budget < 10%",
          "type": "tags"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Error Budget Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "amas_slo_error_budget_remaining_percent",
            "legendFormat": "{{slo_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 10, "color": "orange"},
                {"value": 25, "color": "yellow"},
                {"value": 50, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Error Budget Burn Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "amas_slo_error_budget_burn_rate",
            "legendFormat": "{{slo_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "title": "SLO Compliance Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "amas_slo_compliance",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "amas_slo_current_value",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "amas_slo_error_budget_remaining_percent",
            "format": "table",
            "refId": "C"
          }
        ]
      },
      {
        "id": 4,
        "title": "SLO Violations Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(amas_slo_violations_total[5m])",
            "legendFormat": "{{slo_name}} - {{severity}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      }
    ]
  },
  "resource_dashboard": {
    "title": "AMAS Resource Utilization",
    "tags": ["amas", "resources", "system"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 38,
    "version": 1,
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "enable": true,
          "expr": "(process_resident_memory_bytes / process_virtual_memory_max_bytes) * 100 > 80",
          "iconColor": "orange",
          "name": "High Memory Usage",
          "showIn": 0,
          "step": "1m",
          "titleFormat": "Memory > 80%",
          "type": "tags"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Active Agents",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "amas_active_agents_current",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"}
          }
        }
      },
      {
        "id": 2,
        "title": "Queue Depth",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 0},
        "targets": [
          {
            "expr": "amas_queue_depth_current",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 25, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 16, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "(process_resident_memory_bytes / process_virtual_memory_max_bytes) * 100",
            "legendFormat": "Memory Usage %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 75, "color": "yellow"},
                {"value": 85, "color": "orange"},
                {"value": 95, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 16, "x": 16, "y": 6},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "palette-classic"}
          }
        }
      }
    ]
  }
}
